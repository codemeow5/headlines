{
    "manifest_version": 2,
    "name": "Headlines",
    "description": "Replace your new tab page with the latest news based on the sources you choose.",
    "version": "0.2.26",
    "browser_action": {
      "default_icon": {
        "16": "./icons/icon-16.png",
        "32": "./icons/icon-32.png"
      }
    },
    "content_security_policy": "script-src 'self' https://unpkg.com/feather-icons/dist/feather.min.js; object-src 'self'",
    "icons": {
      "16": "./icons/icon-16.png",
      "32": "./icons/icon-32.png",
      "48": "./icons/icon-48.png",
      "96": "./icons/icon-96.png",
      "128": "./icons/icon-128.png",
      "256": "./icons/icon-256.png"
    },
    "chrome_url_overrides" : {
      "newtab": "index.html"
    },
    "background": {
      "scripts": ["background.js"]
    },    
    "permissions": [
      "topSites",
      "geolocation",
      "tabs"
    ],
    "author": "Juwan Petty <jchpetty@gmail.com> (juwanpetty.com)"
  }